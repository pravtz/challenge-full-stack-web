generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Student {
  ra    Int @id @default(autoincrement())
  name  String @db.VarChar(255)
  email String @unique @db.VarChar(255)
  cpf   String @unique @db.Char(14)

  @@map("student")
}